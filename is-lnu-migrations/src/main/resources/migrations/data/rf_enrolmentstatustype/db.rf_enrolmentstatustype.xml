<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<databaseChangeLog 
	xmlns="http://www.liquibase.org/xml/ns/dbchangelog" 
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" 
	xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-3.0.xsd">
    
    <changeSet id="add q_rf_enrolmentstatustype" author="illay">
    	<insert tableName="q_rf_enrolmentstatustype">
    		<column name="id">1</column>
    		<column name="abbrname"></column>
    		<column name="name">Нова заява</column>
    		<column name="description">заяву (електронну або паперову) прийнято до розгляду у ВНЗ, заведено особову справу вступника з персональним номером та розпочато процес щодо прийняття рішення про допуск вступника до участі у конкурсному відборі</column>
    	    <column name="status">Active</column>
            <column name="actual">1</column>
            <column name="crtuser">illay</column>
            <column name="crtusergroup">create</column>
         	<column name="update_date">now()</column>
            <column name="create_date">now()</column>
       	</insert>
		<insert tableName="q_rf_enrolmentstatustype">
    		<column name="id">2</column>
    		<column name="abbrname"></column>
    		<column name="name">Відмова</column>
    		<column name="description">власника зареєстрованої заяви не допущено до участі у конкурсному відборі на підставі рішення приймальної комісії. У разі присвоєння заяві цього статусу ВНЗ зазначає причину відмови</column>
    	    <column name="status">Active</column>
            <column name="actual">1</column>
            <column name="crtuser">illay</column>
            <column name="crtusergroup">create</column>
         	<column name="update_date">now()</column>
            <column name="create_date">now()</column>
       	</insert>
		<insert tableName="q_rf_enrolmentstatustype">
    		<column name="id">3</column>
    		<column name="abbrname"></column>
    		<column name="name">Скасовано</column>
    		<column name="description">подана заява вважається такою, що не подавалась, а факт подачі – анулюється в ЄДЕБО, якщо: – електронну заяву скасовано вступником в особистому електронному кабінеті до моменту встановлення заяві статусу «Зареєстровано у вищому навчальному закладі» або «Потребує уточнення вступником»; – заяву анульовано ВНЗ за рішенням приймальної комісії (до моменту встановлення статусу «Рекомендовано до зарахування») за умови виявлення ВНЗ технічної помилки, зробленої під час внесення даних до ЄДЕБО, з обов’язковим зазначенням причини анулювання</column>
    	    <column name="status">Active</column>
            <column name="actual">1</column>
            <column name="crtuser">illay</column>
            <column name="crtusergroup">create</column>
         	<column name="update_date">now()</column>
            <column name="create_date">now()</column>
       	</insert>
		<insert tableName="q_rf_enrolmentstatustype">
    		<column name="id">4</column>
    		<column name="abbrname"></column>
    		<column name="name">Допущено</column>
    		<column name="description">власника зареєстрованої заяви допущено до участі у конкурсному відборі</column>
    	    <column name="status">Active</column>
            <column name="actual">1</column>
            <column name="crtuser">illay</column>
            <column name="crtusergroup">create</column>
         	<column name="update_date">now()</column>
            <column name="create_date">now()</column>
       	</insert>
		<insert tableName="q_rf_enrolmentstatustype">
    		<column name="id">5</column>
    		<column name="abbrname"></column>
    		<column name="name">Рекомендовано</column>
    		<column name="description">вступник пройшов конкурсний відбір та рекомендований до зарахування на навчання за кошти державного бюджету або за кошти фізичних та юридичних осіб</column>
    	    <column name="status">Active</column>
            <column name="actual">1</column>
            <column name="crtuser">illay</column>
            <column name="crtusergroup">create</column>
         	<column name="update_date">now()</column>
            <column name="create_date">now()</column>
       	</insert>
		<insert tableName="q_rf_enrolmentstatustype">
    		<column name="id">6</column>
    		<column name="abbrname"></column>
    		<column name="name">Відхилено</column>
    		<column name="description">вступник втратив право бути зарахованим на навчання до обраного ВНЗ у зв’язку з невиконанням вимог  Умов прийому або їх порушенням, зарахуванням на навчання до іншого навчального закладу тощо. При встановленні заяві такого статусу ВНЗ обов’язково зазначає причину виключення</column>
    	    <column name="status">Active</column>
            <column name="actual">1</column>
            <column name="crtuser">illay</column>
            <column name="crtusergroup">create</column>
         	<column name="update_date">now()</column>
            <column name="create_date">now()</column>
       	</insert>
    	<insert tableName="q_rf_enrolmentstatustype">
    		<column name="id">7</column>
    		<column name="abbrname"></column>
    		<column name="name">До наказу</column>
    		<column name="description">наказом про зарахування на навчання вступника зараховано до ВНЗ</column>
    	    <column name="status">Active</column>
            <column name="actual">1</column>
            <column name="crtuser">illay</column>
            <column name="crtusergroup">create</column>
         	<column name="update_date">now()</column>
            <column name="create_date">now()</column>
       	</insert>
 		<insert tableName="q_rf_enrolmentstatustype">
    		<column name="id">8</column>
    		<column name="abbrname"></column>
    		<column name="name">Заява надійшла з сайту</column>
    		<column name="description">підтвердження факту подання електронної заяви до обраного вступником ВНЗ. Подана електронна заява одразу відображається в ЄДЕБО на сторінці ВНЗ</column>
    	    <column name="status">Active</column>
            <column name="actual">1</column>
            <column name="crtuser">illay</column>
            <column name="crtusergroup">create</column>
         	<column name="update_date">now()</column>
            <column name="create_date">now()</column>
       	</insert>
    	<insert tableName="q_rf_enrolmentstatustype">
    		<column name="id">9</column>
    		<column name="abbrname"></column>
    		<column name="name">Затримано</column>
    		<column name="description">електронну заяву прийнято до розгляду у ВНЗ, але дані стосовно вступника потребують уточнення. Після присвоєння електронній заяві цього статусу ВНЗ зобов`язаний невідкладно надіслати вступнику повідомлення з переліком даних, які потребують уточнення, та в який спосіб їх необхідно подати</column>
    	    <column name="status">Active</column>
            <column name="actual">1</column>
            <column name="crtuser">illay</column>
            <column name="crtusergroup">create</column>
         	<column name="update_date">now()</column>
            <column name="create_date">now()</column>
       	</insert>
    </changeSet>
</databaseChangeLog>
