<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<databaseChangeLog
        xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-3.0.xsd">
    <changeSet id="add data rf_persontype" author="illay">
    	<insert tableName="q_rf_persontype">
            <column name="abbrname"></column>
            <column name="name">абітурієнт</column>
            <column name="uid"></column>
            <column name="utid"></column>
            <column name="uapp"></column>
            <column name="status">ACTIVE</column>
            <column name="actual">1</column>
            <column name="note"></column>
            <column name="crtuser">illay</column>
            <column name="crtusergroup">public</column>
            <column name="create_date">now()</column>
            <column name="update_date">now()</column>
        </insert>
    </changeSet>
    <changeSet id="remove item абітурієнт" author="illay">
    	<delete tableName="q_rf_persontype">
    		<where>name='абітурієнт' and abbrname is null</where>
    	</delete>
    </changeSet>
</databaseChangeLog>