    <changeSet author="illay" id="Створення таблиці часових періодів(рік, семестр, місяць)">
        <createTable remarks="Часові періоди (застосовується для систематизації об'єктів бази даних, наприклад таких як документи, по відношенню їх до конкретного навчального чи календарного року, семестру, чверті і т.д.)" tableName="q_ob_timeperiod">
            <column name="id" remarks="Ідентифікатор" type="BIGINT">
                <constraints nullable="false"/>
            </column>
            <column name="timeperiodtype_id" remarks="Ідентифікатор типу часового періоду" type="BIGINT">
                <constraints nullable="false"/>
            </column>
            <column name="name" remarks="Текстове позначення часового періоду (Наприклад: 2013-2014 для 2013 н.р.; 2013 для 2013 календарного року)" type="VARCHAR(50)">
                <constraints nullable="false"/>
            </column>
            <column name="numvalue" remarks="Числове позначення часового періоду (Для календарного року воно співпадає з текстовим позначенням; для навчального року це рік початку начального року)" type="INTEGER">
                <constraints nullable="false"/>
            </column>
            <column name="begdate" type="TIMESTAMP"/>
            <column name="enddate" type="TIMESTAMP"/>
            <column name="parent_id" remarks="Ієрарічний ідентифікатор (Ієрархія: приналежність семестру до року і т.д.)" type="BIGINT"/>
            <column name="uid" remarks="Унікальний ідентифікатор іншої ІС" type="BIGINT"/>
            <column name="utid" remarks="Унікальний текстовий ідентифікатор іншої ІС" type="VARCHAR(50)"/>
            <column name="uapp" remarks="Унікальна інша інформаційна система" type="VARCHAR(50)"/>
            <column name="status" remarks="Ознака статусу запису" type="varchar(50)">
                <constraints nullable="false"/>
            </column>
            <column name="actual" remarks="Актуальність даних" type="INTEGER">
                <constraints nullable="false"/>
            </column>
            <column name="note" remarks="Примітка" type="VARCHAR(250)"/>
            <column name="crtuser" remarks="Користувач, що вніс зміни (значення за замовчуванням)" type="VARCHAR(50)">
                <constraints nullable="false"/>
            </column>
            <column name="crtusergroup" remarks="Група до якої відноситься користувач, що вніс зміни (значення за замовчуванням)" type="VARCHAR(50)">
                <constraints nullable="false"/>
            </column>
            <column name="update_date" remarks="Дата внесення змін (значення за замовчуванням)" type="TIMESTAMP">
                <constraints nullable="false"/>
            </column>
            <column name="create_date" remarks="Дата створення" type="TIMESTAMP">
            	<constraints nullable="false"/>
        	</column>
        </createTable>
    
    	<addPrimaryKey columnNames="id" constraintName="pk_q_ob_timeperiod_id" tableName="q_ob_timeperiod"/>
    	
    	<addForeignKeyConstraint baseColumnNames="parent_id" baseTableName="q_ob_timeperiod" constraintName="fk_q_ob_timeperiod_id" deferrable="false" initiallyDeferred="false" onDelete="NO ACTION" onUpdate="NO ACTION" referencedColumnNames="id" referencedTableName="q_ob_timeperiod"/>
    	
